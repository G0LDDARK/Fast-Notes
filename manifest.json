{
    "manifest_version": 2,
    "name": "Fast Notes",
    "description": "Prise de note rapide pour navigateur !",
    "version": "2.1",
    "permissions": [
    "http://*/*",
    "https://*/*",
      "tabs"
    ],
    "browser_action": {
      "default_icon" : "32.png",
      "default_popup": "index.html"
    },
    "icons": {
      "16" : "16.png",
      "32" : "32.png"
    },
    "background": {
      "scripts": ["app.js"]
    }
  }